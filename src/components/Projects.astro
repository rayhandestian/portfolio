<section class="section" id="projects">
  <div class="container">
    <h2 class="section-title">Projects</h2>
    <p class="section-subtitle">A selection of work I'm proud of.</p>
    
    <div class="featured-group">
      <div class="featured-header">
        <div class="header-content">
          <h3 class="featured-title">Kaizen Network</h3>
          <p class="featured-description">
            Indonesian Minecraft Server Network. Built a full-stack e-commerce website, custom Java Spigot & Velocity plugins, various Skript scripts, Discord bots, and managed cloud infrastructure.
          </p>
        </div>
        <a href="https://kaizenmc.id" target="_blank" rel="noopener" class="btn btn-outline">
          Visit Website
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6"/><polyline points="15 3 21 3 21 9"/><line x1="10" y1="14" x2="21" y2="3"/></svg>
        </a>
      </div>
      
      <div class="projects-grid kaizen-grid">
        {kaizenProjects.map(project => (
          <ProjectCard
            title={project.title}
            description={project.description}
            tags={project.tags}
          />
        ))}
      </div>
    </div>

    <!-- Removed Other Works header -->
    <div class="projects-grid">
      {otherProjects.map(project => (
        <ProjectCard
          title={project.title}
          description={project.description}
          tags={project.tags}
          link={project.link}
          github={project.github}
        />
      ))}
    </div>
  </div>
</section>

<style>
  .projects-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: var(--space-lg);
    justify-content: center;
  }

  .featured-group {
    background: var(--color-bg-card); /* Make it look like a card */
    border: 1px solid var(--color-border);
    border-radius: var(--radius-lg);
    padding: var(--space-xl);
    margin-bottom: var(--space-2xl);
    text-align: left;
    transition: box-shadow 0.2s ease, border-color 0.2s ease;
  }
  
  .featured-group:hover {
    box-shadow: var(--shadow-subtle);
    border-color: var(--color-accent);
  }

  .featured-header {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    margin-bottom: var(--space-xl);
    flex-wrap: wrap;
    gap: var(--space-md);
  }

  .header-content {
    max-width: 600px;
  }

  .featured-title {
    font-size: 1.75rem; /* Larger title for main card */
    color: var(--color-text-primary);
    margin-bottom: var(--space-xs);
    font-weight: 600;
  }

  .featured-description {
    font-size: 1rem;
    color: var(--color-text-secondary);
    line-height: 1.6;
  }

  .section-heading-small {
    font-size: 1.25rem;
    margin-bottom: var(--space-lg);
    color: var(--color-text-primary);
    text-align: left; /* Align header to standard flow */
    padding-left: var(--space-md); /* Match grid padding alignment roughly */
  }
  
  @media (max-width: 768px) {
    .featured-header {
      flex-direction: column;
    }
    
    .featured-group {
      padding: var(--space-md);
    }
    
    .projects-grid {
      grid-template-columns: 1fr;
    }
  }
</style>
